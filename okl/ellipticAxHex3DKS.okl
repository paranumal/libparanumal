#define sliceThickness 1
#elif p_Nq <= 13
#define sliceThickness 2
#elif p_Nq p_Nq == 14
#define sliceThickness 3
#elif p_Nq <=16
#define sliceThickness 4
#else
#define sliceThickness 6
#endif

#if p_Nq < 11
#define shift p_Nq
#else
#define shift  1024/(p_Nq*p_Nq)
#endif

