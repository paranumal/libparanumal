        s_q[p_Nfields*n+2] = px+py;
      }
    }
    @barrier("local");
    
    for(int n=0; n < p_maxNodes; ++n; @inner(0)){ 
      if (n<p_Nfaces*p_Nfp) {
        const int e = pmlElementIds[et];
        const int f = n / p_Nfp;
