      for(int j=0;j<p_cubNq;++j;@inner(1)){
        for(int i=0;i<p_cubNq;++i;@inner(0)){

          for(int fld=0; fld<p_Nrelax; fld++) {
            r_q[fld] = 0.f;
            r_qx[fld] = 0.f;
            r_qy[fld] = 0.f;
            r_qz[fld] = 0.f;
          }

